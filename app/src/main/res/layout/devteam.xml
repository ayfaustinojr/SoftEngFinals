<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical" android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/fakemoneyscanner_ui_non2">

    <ImageView
        android:layout_width="70dp"
        android:layout_height="70dp"
        android:src="@drawable/i2"
        android:id="@+id/imageView"
        android:layout_marginStart="30dp"
        android:layout_marginTop="90dp"
        android:layout_alignParentTop="true"
        android:layout_alignParentStart="true" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/textView5"
        android:layout_alignStart="@+id/textView5"
        android:layout_marginTop="13dp"
        android:id="@+id/textView6"
        android:textColor="@android:color/black"
        android:text="Jane P. Balboa" />

    <TextView
        android:text='"Scrumbled Egg Master"'
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="19dp"
        android:id="@+id/textView5"
        android:textColor="@android:color/black"
        android:layout_alignTop="@+id/imageView"
        android:layout_toEndOf="@+id/imageView" />

    <ImageView
        android:layout_width="70dp"
        android:layout_height="70dp"
        android:src="@drawable/gallery"
        android:layout_below="@+id/imageView"
        android:layout_alignStart="@+id/imageView"
        android:layout_marginTop="44dp"
        android:id="@+id/imageView2" />

    <TextView
        android:text='"Kink Master 3000"'
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignTop="@+id/imageView2"
        android:layout_alignStart="@+id/textView6"
        android:id="@+id/textView7"
        android:textColor="@android:color/black" />

    <TextView
        android:text="Jhance Bredh C. Laya"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/textView7"
        android:layout_alignStart="@+id/textView7"
        android:layout_marginTop="14dp"
        android:id="@+id/textView8"
        android:textColor="@android:color/black" />

    <ImageView
        android:layout_width="70dp"
        android:layout_height="70dp"
        android:src="@drawable/camera"
        android:id="@+id/imageView12"
        android:layout_marginTop="44dp"
        android:layout_below="@+id/imageView2"
        android:layout_alignStart="@+id/imageView2" />

    <ImageView
        android:layout_width="70dp"
        android:layout_height="70dp"
        android:src="@drawable/camera"
        android:id="@+id/imageView13"
        android:layout_marginTop="44dp"
        android:layout_below="@+id/imageView12"
        android:layout_alignStart="@+id/imageView12" />

    <TextView
        android:text='""'
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/textView9"
        android:textColor="@android:color/black"
        android:layout_alignTop="@+id/imageView12"
        android:layout_alignStart="@+id/textView8" />

    <TextView
        android:text="Angelica L. Salazar"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="15dp"
        android:id="@+id/textView10"
        android:textColor="@android:color/black"
        android:layout_below="@+id/textView9"
        android:layout_alignStart="@+id/textView9" />

    <TextView
        android:text='""'
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textColor="@android:color/black"
        android:layout_alignTop="@+id/imageView13"
        android:layout_alignStart="@+id/textView10"
        android:id="@+id/textView13" />

    <TextView
        android:text="Arthur Y. Faustino Jr."
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="14dp"
        android:textColor="@android:color/black"
        android:layout_below="@+id/textView13"
        android:layout_alignStart="@+id/textView13"
        android:id="@+id/textView14" />

</RelativeLayout>